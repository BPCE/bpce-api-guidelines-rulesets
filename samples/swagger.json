  "definitions": {
    "CheckOtp": {
      "type": "object",
      "required": ["codeOtp", "recipientType"],
      "properties": {
        "codeOtp": {
          "type": "string",
          "example": 123456,
          "description": "Code otp",
          "minLength": 4,
          "maxLength": 8
        },
        "recipientType": {
          "type": "string",
          "example": "EP",
          "description": "référentiel",
          "enum": ["SAVER", "CORRESPONDENT"]
        }
      },
      "title": "CheckOtp"
    },
    "Email": {
      "type": "object",
      "required": ["type"],
      "properties": {
        "data": {
          "type": "object",
          "description": "Informations complémentaires à l'envoi de mail"
        },
        "type": {
          "type": "string",
          "example": "UPDATE_RESCUE_EMAIL",
          "description": "Type de mail à envoyer",
          "enum": [
            "UPDATE_EMAIL",
            "UPDATE_RESCUE_EMAIL",
            "UPDATE_SECRET_QUESTION",
            "UPDATE_PASSWORD",
            "UPDATE_MOBILE_PHONE_NUMBER",
            "UPDATE_BANKING_INFORMATION",
            "UPDATE_POSTAL_DETAILS",
            "DELETE_REDEMPTION",
            "DELETE_RELEASE",
            "DELETE_TRANSFER_BETWEEN_PRODUCTS",
            "DELETE_TRANSFER",
            "CREATE_REDEMPTION",
            "CREATE_RELEASE",
            "CREATE_COMPLEMENT_RELEASE",
            "CREATE_REPLY_COMPLEMENTARY_PROFIT_SHARING",
            "CREATE_REPLY_MANDATORY_PROFIT_SHARING",
            "CREATE_REPLY_AVAILABILITY",
            "CREATE_TRANSFER_BETWEEN_PRODUCTS",
            "CREATE_TRANSFER",
            "AMPLUS_WELCOME",
            "AMPLUS_RECOVERY_CREATE_STATUS",
            "AMPLUS_RECOVERY_SAVE_STATUS",
            "AMPLUS_CONGRATULATIONS",
            "AMPLUS_ACTION_FOLLOW_UP"
          ]
        }
      },
      "title": "Email",
      "description": "Demande d'envoi d'un certain type de mail"
    },
    "EmailDeprecated": {
      "type": "object",
      "properties": {
        "applicationType": {
          "type": "string",
          "example": "EP",
          "description": "Type application appelante : EP, EE, AMPLUS, ACTIRIS, EASIRIS)",
          "minLength": 0,
          "maxLength": 10
        },
        "companyNumber": {
          "type": "integer",
          "format": "int32",
          "description": "Numéro entreprise"
        },
        "creationDate": {
          "type": "string",
          "format": "date-time",
          "description": "Date de création du mail"
        },
        "emailAddress": {
          "type": "string",
          "description": "Adresse email du destinataire",
          "minLength": 0,
          "maxLength": 100
        },
        "externalId": {
          "type": "integer",
          "format": "int64",
          "description": "Identifiant unique externe"
        },
        "from": {
          "type": "string",
          "description": "L'émetteur du mail",
          "minLength": 0,
          "maxLength": 255
        },
        "jsonData": {
          "type": "object",
          "description": "Paramètres spécifiques au mail"
        },
        "language": {
          "type": "string",
          "example": "FR",
          "description": "Code langue du mail",
          "minLength": 0,
          "maxLength": 3
        },
        "mailType": { "type": "integer", "format": "int32", "example": 5 },
        "recipientNumber": {
          "type": "string",
          "description": "Numéro destinataire",
          "minLength": 0,
          "maxLength": 15
        },
        "recipientType": {
          "type": "string",
          "description": "Type de destinataire : EP, CRP",
          "minLength": 0,
          "maxLength": 10
        },
        "requestSubtype": { "type": "string", "minLength": 0, "maxLength": 10 },
        "requestType": { "type": "string", "minLength": 0, "maxLength": 10 },
        "templateCode": {
          "type": "string",
          "example": "OUV_BO",
          "minLength": 0,
          "maxLength": 10
        },
        "templateType": {
          "type": "string",
          "example": "NOTIFICATION",
          "minLength": 0,
          "maxLength": 30
        }
      },
      "title": "EmailDeprecated",
      "description": "Les infos du mail (destinataire, gabarits, parametres)"
    },
    "Error": {
      "type": "object",
      "required": ["code", "message"],
      "properties": {
        "additionalProperties": {
          "type": "object",
          "description": "propriétés additionnelles spécifiques"
        },
        "attribute": {
          "type": "string",
          "description": "nom de l'attribut source de l'erreur"
        },
        "code": { "type": "string", "description": "le code de l'erreur" },
        "message": {
          "type": "string",
          "description": "description de l'erreur"
        },
        "path": {
          "type": "string",
          "description": "JSON path de l'attribut en erreur"
        }
      },
      "title": "Error"
    },
    "Errors": {
      "type": "object",
      "properties": {
        "errors": {
          "type": "array",
          "items": { "$ref": "#/definitions/Error" }
        }
      },
      "title": "Errors"
    },
    "Otp": {
      "type": "object",
      "required": [
        "applicationCode",
        "channel",
        "companyNumber",
        "recipientNumber"
      ],
      "properties": {
        "applicationCode": {
          "type": "string",
          "example": "ACTIRIS",
          "description": "Type application appelante : EP, EE, AMPLUS, ACTIRIS, EASIRIS)",
          "minLength": 0,
          "maxLength": 10
        },
        "channel": {
          "type": "string",
          "example": "SMS",
          "description": "Type d'envoi du SMS",
          "enum": ["SMS", "VOICE", "EMAIL"]
        },
        "companyNumber": {
          "type": "integer",
          "format": "int32",
          "description": "Numéro entreprise"
        },
        "context": {
          "type": "string",
          "example": "REINIT_MDP",
          "description": "Contexte de l'otp",
          "minLength": 0,
          "maxLength": 10
        },
        "email": {
          "type": "string",
          "description": "Adresse email du destinataire",
          "minLength": 0,
          "maxLength": 100
        },
        "expiresIn": {
          "type": "integer",
          "format": "int32",
          "description": "Durée de validité en minutes, par défaut à 10 minutes",
          "minimum": 1.0,
          "exclusiveMinimum": false
        },
        "language": {
          "type": "string",
          "example": "FR",
          "description": "Code langue de l'otp"
        },
        "phone": {
          "type": "string",
          "example": "(+33)612345678",
          "description": "Numero de téléphone",
          "minLength": 0,
          "maxLength": 20
        },
        "recipientNumber": {
          "type": "string",
          "example": 273261,
          "description": "Numéro du salarié ou du correspondant"
        }
      },
      "title": "Otp"
    },
    "OtpSms": {
      "type": "object",
      "required": [
        "language",
        "phoneNumber",
        "recipientNumber",
        "section",
        "sendType"
      ],
      "properties": {
        "companyNumber": {
          "type": "integer",
          "format": "int32",
          "example": 9999,
          "description": "Numéro d'entreprise (pour les epargnants)",
          "minimum": 0.0,
          "maximum": 999999999,
          "exclusiveMinimum": false,
          "exclusiveMaximum": false
        },
        "language": {
          "type": "string",
          "example": "FR",
          "description": "Code langue de l'otp"
        },
        "phoneNumber": {
          "type": "string",
          "example": "(+33)649930234",
          "description": "Numero de téléphone",
          "minLength": 0,
