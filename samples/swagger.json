            "examples": {
              "InvalidFormat": "{\n  \"errors\" : [ {\n    \"code\" : \"INVALID_FORMAT\",\n    \"message\" : \"Invalid format\",\n    \"attribute\" : \"zip\",\n    \"path\" : \"$.addresses[2].zip\"\n  } ]\n}",
              "OtpsmsExpired": "{\n  \"errors\" : [ {\n    \"code\" : \"OTPSMS_EXPIRED\",\n    \"message\" : \"otp expired\"\n  } ]\n}",
              "Required": "{\n  \"errors\" : [ {\n    \"code\" : \"REQUIRED\",\n    \"message\" : \"may not be null\",\n    \"attribute\" : \"zip\",\n    \"path\" : \"$.addresses[2].zip\"\n  } ]\n}",
              "WrongOtp": "{\n  \"errors\" : [ {\n    \"code\" : \"WRONG_OTP\",\n    \"message\" : \"Invalid otp\"\n  } ]\n}"
            }
