{
  "swagger": "2.0",
  "info": {
    "description": "Cette API vous permet de notifier un utilisateur (envoi de mail codes OTP par mail ou message vocal).",
    "version": "0.0.1",
    "title": "notification-api-v1"
  },
  "host": "nie.api.dev.intranatixis.com",
  "basePath": "/dev2/notification",
  "tags": [
    { "name": "Email", "description": "Send email" },
    { "name": "OTP SMS", "description": "Otp Sms Controller" },
    { "name": "OTPS", "description": "Otp Controller" }
  ],
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "paths": {
    "/v1/email": {
      "post": {
        "tags": ["Email"],
        "summary": "Deprecated: use /v1/employee/{employeeId}/emails",
        "description": "Permet d’envoyer un email en temps réel, il faut spécifier les infos du mail (destinataire, gabarits, parametres).",
        "operationId": "sendEmailUsingPOST_1",
        "parameters": [
          {
            "in": "body",
            "name": "email",
            "description": "email",
            "required": true,
            "schema": { "$ref": "#/definitions/EmailDeprecated" }
          }
        ],
        "responses": {
          "201": {
            "description": "Created",
            "schema": { "$ref": "#/definitions/Tracking" }
          },
          "202": {
            "description": "Une erreur est survenue lors de l'envoi du mail. Une nouvelle tentative d'envoi sera effectué automatiquement.",
            "schema": { "$ref": "#/definitions/Tracking" }
          },
          "400": {
            "description": "Il y a une(des) erreur(s) dans la requête.",
            "schema": { "$ref": "#/definitions/Errors" },
            "examples": {
              "InvalidFormat": "{\n  \"errors\" : [ {\n    \"code\" : \"INVALID_FORMAT\",\n    \"message\" : \"Invalid format\",\n    \"attribute\" : \"zip\",\n    \"path\" : \"$.addresses[2].zip\"\n  } ]\n}",
              "Required": "{\n  \"errors\" : [ {\n    \"code\" : \"REQUIRED\",\n    \"message\" : \"may not be null\",\n    \"attribute\" : \"zip\",\n    \"path\" : \"$.addresses[2].zip\"\n  } ]\n}"
            }
          },
          "401": {
            "description": "Utilisateur non authentifié.",
            "schema": { "$ref": "#/definitions/Errors" },
            "examples": {
              "Unauthorized": "{\n  \"errors\" : [ {\n    \"code\" : \"UNAUTHORIZED\",\n    \"message\" : \"Valid credentials required\"\n  } ]\n}"
            }
          },
          "403": {
            "description": "Utilisateur authentifié mais droits insuffisants.",
            "schema": { "$ref": "#/definitions/Errors" },
            "examples": {
              "Forbidden": "{\n  \"errors\" : [ {\n    \"code\" : \"FORBIDDEN\",\n    \"message\" : \"Access is forbidden\"\n  } ]\n}"
            }
          },
          "500": {
            "description": "Une erreur inattendue s'est produite.",
            "schema": { "$ref": "#/definitions/Errors" },
            "examples": {
              "Unexpected": "{\n  \"errors\" : [ {\n    \"code\" : \"UNEXPECTED\",\n    \"message\" : \"Unexpected error\"\n  } ]\n}"
            }
          }
        },
        "security": [{ "notification_auth": ["sendNotification"] }],
        "deprecated": true
      }
    },
